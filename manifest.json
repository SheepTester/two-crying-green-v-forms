{
  "name": "Two Crying Green V Forms",
  "description": "Track and plan your UCSD dining dollar usage",
  "version": "0.1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["*://eacct-ucsd-sp.transactcampus.com/eAccounts/*"],
      "css": ["/dist/content-styles.css"],
      "js": ["/dist/content-script.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["/dist/isolation-escape.js"],
      "matches": ["*://eacct-ucsd-sp.transactcampus.com/*"]
    }
  ],
  "icons": {
    "128": "/images/icon128.png"
  }
}
